blocks
let e = 2.71828
let R = 20000
let C = 2e-10
let Vc = 0
let Vin = 3.3
let t = 0
for (let i = 0; i < 75; i++) {
    Vc = Vin * (1 - e ** (t / (R * C)))
    t += -0.0000005
    console.logValue("Vc", Vc)
    pause(100)
}
t = 0
Vin = Vc
for (let i = 0; i < 75; i++) {
    Vc = Vin * (e ** (t / (R * C)))
    t += -0.0000005
    console.logValue("Vc", Vc)
    pause(100)
} 
